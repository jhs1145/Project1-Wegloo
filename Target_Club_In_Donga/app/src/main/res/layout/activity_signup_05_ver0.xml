<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/signup_05_artborad"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/artboard_main_ver0">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.850045998"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.8684375"
        android:layout_height="0dp"
        app:layout_constraintVertical_bias="1"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        >

        <View
            android:id="@+id/signup_05_artborad_sub"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/artboard_sub_su_allradius_ver0"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintHeight_percent="0.7834375"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.402864583">
        </View>

        <ImageView
            android:id="@+id/signup_05_imageview_ai"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintHeight_percent="0.654895833"
            app:layout_constraintVertical_bias="0.860989583"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/wegloo_we_lierste" />

        <com.jetradarmobile.snowfall.SnowfallView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="20dp"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintWidth_percent="0.8"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintHeight_percent="0.5"
            app:snowflakesFadingEnabled="true"
            app:snowflakesAlreadyFalling="true"
            app:snowflakesNum="35"
            app:snowflakeImage="@drawable/ic_snow_circle"
            app:snowflakeSizeMin="15dp"
            app:snowflakeSizeMax="32dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>


    <ImageView
        android:id="@+id/signup_05_imageview_logo"
        android:layout_width="0dp"

        android:layout_height="0dp"
        android:background="@drawable/wegloo2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.066666667"
        app:layout_constraintHorizontal_bias="0.364305428"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.906041667"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.420423183" />

    <TextView
        android:id="@+id/signup_05_textview_wefriend"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeMaxTextSize="20dp"
        android:autoSizeMinTextSize="1sp"
        android:autoSizeTextType="uniform"
        android:gravity="center_horizontal"

        android:text="의 친구가"
        android:textColor="@color/color_White"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.040104167"
        app:layout_constraintHorizontal_bias="0.858"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.896"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.313827047" />

    <TextView
        android:id="@+id/signup_05_textview_thanks"
        android:text="되어주셔서 감사합니다."
        android:gravity="center_horizontal"
        android:textColor="@color/color_White"
        android:autoSizeMinTextSize="1sp"
        android:autoSizeMaxTextSize="20dp"
        android:autoSizeTextType="uniform"

        android:layout_width="0dp"
        app:layout_constraintWidth_default="percent"
        app:layout_constraintWidth_percent="0.48574057"
        app:layout_constraintHorizontal_bias="0.5"
        android:layout_height="0dp"
        app:layout_constraintHeight_default="percent"
        app:layout_constraintHeight_percent="0.0375"
        app:layout_constraintVertical_bias="0.95390625"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>